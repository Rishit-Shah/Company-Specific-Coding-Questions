class Solution {
    public boolean isPalindrome(String s) {
        s=s.toLowerCase();
        s=s.replaceAll("[^a-zA-Z0-9]","");
        s.trim();
   
        StringBuilder reverse= new StringBuilder(s);
        reverse.reverse();
        for(int i=0;i<s.length();i++){
            if(reverse.charAt(i)!=s.charAt(i)){ 
                return false;

            }

        }
        return true;

        
    }
}
